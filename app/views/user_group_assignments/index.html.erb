<p id="notice"><%= notice %></p>

<h1>User Group Assignments</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_group_assignments.each do |user_group_assignment| %>
      <tr>
        <td><%= link_to 'Show', user_group_assignment %></td>
        <td><%= link_to 'Edit', edit_user_group_assignment_path(user_group_assignment) %></td>
        <td><%= link_to 'Destroy', user_group_assignment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

