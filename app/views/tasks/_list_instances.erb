<div class="table-responsive text-center table-striped">
  <table class="table">
    <thead>
      <tr>
        <th>Due date</th>
        <th>Completed by</th>
        <th>Completed on</th>
      </tr>
    </thead>

    <tbody>
      <% instances.each do |instance| %>
        <tr>
          <td><%= instance.due_date %></td>
          <td><%= instance.user.full_name %></td>
          <td><%= instance.updated_at.to_date %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
